<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../stylesheets/style.css">
    <link href="https://cdn.jsdelivr.net/npm/remixicon@2.5.0/fonts/remixicon.css" rel="stylesheet">
   
    <title>Document</title>
</head>
<body>

    <div id="main">
        <div id="left">
            <div id="ltop">
                <img src="../images/zomato.png" alt="" srcset="">
            </div>
            <div id="lmid">
                <div id="profile">
                    <div id="profile_img">
                        <img src="../images/default-user.png" alt="">
                    </div>
                    <div id="profile_dets">
                        <h3>Mark Clarke</h3>
                        <h3>example@gmail.com</h3>
                    </div>
                </div>
               <div id="a">
                <a href="/index"><i class="ri-home-4-line"></i>HOME</a>
                <a href="/explore"><i class="ri-layout-grid-line"></i>EXPLORE</a>
                <a href="/favour"><i class="ri-bookmark-line"></i>FAVOURITES</a>
                <a><i class="ri-file-list-3-line"></i>ORDERS</a>
                <a><i class="ri-message-2-line"></i>MESSAGES</a>
                <a><i class="ri-settings-5-fill"></i>SETTINGS</a>
               </div>
            </div>
        </div>
        <div id="right">
            <div id="rtop">
                <div id="rtopl">
                    <div id="loc">
                       <a href=""> <i class="ri-map-pin-2-line"></i>
                        <h5>San Frnciso, california</h5></a>
                    </div>
                    <div id="pick">
                        <a href=""><i class="ri-shopping-bag-3-fill"></i>
                            <h5>PICK UP</h5></a>
                    </div>
                    <div id="best_deal">
                        <a href=""><i class="ri-vip-crown-2-line"></i>
                            <h5>BEST DEALS</h5></a>
                    </div>
                </div>
                <div id="rtopr">
                    <form action="/index.html" method="post">
                        <input type="text" placeholder="Search for...">
                        <button><i class="ri-search-2-line"></i></button>
                    </form>
                    <div id="rtopr_bot">
                        <button id="filter"><i class="ri-filter-3-line"></i></button>
                    <button id="cart"><i class="ri-shopping-bag-3-line"></i> <span>0</span></button>
                    </div>
                </div>
            </div>

            <div id="explore_cate">
                <div id="ex_txt">
                    <h3>Explore Categories</h3>
                    <a href="">See all ></a>
                </div>
                <div id="exCards">

                </div>
            </div>
            <div id="feat_rest">
                <div id="feat_txt">
                    <h3>Featured restaurants</h3>
                    <a href="">See all ></a>
                </div>
                <div id="featCards">
                   <div id="ffirst">
                    <% val.forEach(function(elem){ %>
                        <div class="fcards">
                            <div id="brandimg">
                                <img src= "<%=elem.image%>" alt="">
                            </div>
                            <div id="fcardsR">
                                <div id="fcardstop"><%=elem.productname%></h4></div>
                                <div id="fcardsmid">
                                     <div id="l"><i class="ri-star-s-fill"></i><%=elem.rating%></div>
                                     <div id="m"><img src="<%=elem.smallImage%>" alt=""><h5><%=elem.product%></h5></div>
                                     <div id="r"><h5>Rs. <%=elem.price%>/-</h5></div>
                                </div>
                                <div id="fcardsbot">
                                    <h5 id="free"><%=elem.tag%></h5>
                                    <div id="floc">
                                        <i class="ri-map-pin-2-line"></i>
                                        <h5><%=elem.km%>km</h5>
                                    </div>
                                </div>
                            </div>
                        </div>      
                       <% }) %>
                   </div>
                   <div id="fsec"></div>
                </div>
            </div>
            <div id="asian">
                <div id="asian_txt">
                    <h3>Asian food</h3>
                    <a href="">See all ></a>
                </div>
                <div id="asianCards">
                   <div id="afirst">
                   </div>
                   <div id="asec"></div>
                </div>
            </div>
        </div>
    </div>
    
    <script src="../javascripts/script.js"></script>
</body>
</html>

